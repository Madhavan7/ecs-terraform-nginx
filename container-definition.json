[
  {
    "name": "app",  
    "image": "499751399505.dkr.ecr.ca-central-1.amazonaws.com/nginx:latest",  
    "cpu": 1024,
    "memory": 2048,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 8081,
        "hostPort": 8081,
        "protocol": "tcp"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/my-task",
        "awslogs-region": "ca-central-1",
        "awslogs-stream-prefix": "ecs"
      }
    }
  }
]